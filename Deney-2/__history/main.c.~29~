#include <16F877A.h>

#FUSES NOWDT, HS
#use delay(clock=20000000)
void main()
{  
   setup_adc_ports(NO_ANALOGS);
   setup_adc(ADC_CLOCK_DIV_2);
   
  
   int sayi = 0;
   int pinsag = 64;
   int pinsol = 71;
   

   while(sayi <= 7 ){
   
      output_high(64);
      output_high(71);
      delay_ms(100);
      pinsag = pinsag +1;
      pinsol = pinsol -1;
      if(pinsag == 71 && pinsol == 64){
      
      }
      
      }
   }  

}
