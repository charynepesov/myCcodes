#include <16F877A.h>

#FUSES NOWDT, HS
#use delay(clock=20000000)
void main()
{  
   setup_adc_ports(NO_ANALOGS);
   setup_adc(ADC_CLOCK_DIV_2);
   
   output_d(0);
   int sayi = 0;
   

   while(sayi <= 7){
   
      output_high(pin_0);
      delay_ms(1000);
     
      output_low(pin_0);
      delay_ms(1000);
      sayi ++;
      
      
      
        

}
